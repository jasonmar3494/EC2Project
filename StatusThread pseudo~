import java.util.*;


public class StatusThread implements Runnable{
	

	public StatusThread(){
	}
		
	public void run(){
		try{
			
			while(tasks still running){					
				for(int i=0;i<number of nodes currently on map 		
				gets instance name/ID and makes a new entry in the map with 					executing/free
				completion status 
				
			}		
		}
		catch(Exception e){}
	}
}
